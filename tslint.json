{
  "extends": ["tslint:latest"],
  "rules": {
    "no-var-keyword": true,
    "member-access": false,
    "quotemark": [true, "single"],
    "no-console": [false],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "max-line-length": false,
    "no-empty": false,
    "variable-name": false,
    "interface-name": false,
    "class-name": false,
    "no-empty-interface": false
  }
}
